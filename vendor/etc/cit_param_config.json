{
    "version":"1.0",
    "product":"ares",
    "testitems":[
        {
            "name":"home_version_info_test",
            "caption":"版本信息测试"

        },
        {
            "name":"home_sim_card_check_test",
            "caption":"SIM卡测试"

        },
        {
            "name":"home_vibrator_check_test",
            "caption":"震动测试"

        },
        {
            "name":"home_keyboard_check_test",
            "caption":"按键测试",
            "config":{
                "support_check_keys": ["volumeUp_down","volumeUp_up",
                    "volumeDown_down","volumeDown_up", "power_down","power_up",
                    "left_game_key_down","left_game_key_up","right_game_key_down","right_game_key_up",
                    "right_shoulder_switch_on","right_shoulder_switch_off","left_shoulder_switch_on","left_shoulder_switch_off"]
            }
        },
        {
            "name": "home_led_test",
            "caption": "LED灯测试",
            "config": {
                "support_checked_leds": [
                    {
                        "name": "cit_lcdlight_brightness",
                        "type": "screen_brightness",
                        "max_brightness":2047,
                        "breath":true
                    },
                    {
                        "name": "cit_torch_first",
                        "type": "camera_flash",
                        "switch_node_path": "/sys/class/leds/led:switch_0",
                        "torch_node_path": "/sys/class/leds/led:torch_0"
                    },
                    {
                        "name": "cit_torch_second",
                        "type": "camera_flash",
                        "switch_node_path": "/sys/class/leds/led:switch_1",
                        "torch_node_path": "/sys/class/leds/led:torch_1"
                    },
                    {
                      "name": "cit_batterylight_red",
                      "type": "power_led",
                      "torch_node_path": "/sys/class/leds/red",
                      "max_brightness": 255
                    },
                    {
                      "name": "cit_batterylight_blue",
                      "type": "power_led",
                      "max_brightness": 255,
                      "torch_node_path": "/sys/class/leds/blue"
                    },
                    {
                      "name": "cit_batterylight_green",
                      "type": "power_led",
                      "max_brightness": 255,
                      "torch_node_path": "/sys/class/leds/green"
                    }
                ]
            }
        },
        {
            "name":"home_touch_panel_test",
            "caption":"触摸屏测试"

